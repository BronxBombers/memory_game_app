<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.zach.memorygame.LevelClasses.S3L2"
    android:theme="@style/LB_100"
    android:id="@+id/stage2level2_layout">

    <Button
        android:id="@+id/S2L2_B1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/S2L2_B9"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/S2L2_B9"
        app:layout_constraintTop_toBottomOf="@+id/S2L2_B10"
        app:layout_constraintVertical_bias="0.32" />

    <Button
        android:id="@+id/S2L2_B2"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/S2L2_B3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/S2L2_B7"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/S2L2_B7"
        app:layout_constraintTop_toBottomOf="@+id/S2L2_B8"
        app:layout_constraintVertical_bias="0" />

    <Button
        android:id="@+id/S2L2_B4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/S2L2_B3"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/S2L2_B3"
        app:layout_constraintTop_toBottomOf="@+id/S2L2_B5"
        app:layout_constraintVertical_bias="0.32" />

    <Button
        android:id="@+id/S2L2_B5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/S2L2_B8"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/S2L2_B8"
        app:layout_constraintTop_toBottomOf="@+id/S2L2_B3"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/S2L2_B6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/S2L2_B2"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/S2L2_B7"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0" />

    <Button
        android:id="@+id/S2L2_B8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/S2L2_B7"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/S2L2_B9"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/S2L2_B2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/S2L2_B2"
        app:layout_constraintTop_toBottomOf="@+id/S2L2_B6"
        app:layout_constraintVertical_bias="0" />

    <Button
        android:id="@+id/S2L2_B10"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="@+id/S2L2_B5"
        app:layout_constraintEnd_toStartOf="@+id/S2L2_B8"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/S2L2_B6"
        app:layout_constraintTop_toBottomOf="@+id/S2L2_B9" />
    <Button
        android:id="@+id/cardSwapPopUp"
        android:layout_width="324dp"
        android:layout_height="386dp"
        android:background="@drawable/cartoon_popup_text_holder"
        android:gravity="center"
        android:text="Card Swap!"
        android:textSize="72sp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</android.support.constraint.ConstraintLayout>
