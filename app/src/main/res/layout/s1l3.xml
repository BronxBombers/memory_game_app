    <Button
        android:id="@+id/S1L3_B9"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="52dp"
        app:layout_constraintEnd_toEndOf="@+id/S1L3_B6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/S1L3_B6"
        app:layout_constraintTop_toBottomOf="@+id/S1L3_B6" />

    <Button
        android:id="@+id/S1L3_B10"
        app:layout_constraintEnd_toEndOf="@+id/S1L3_B8"
        app:layout_constraintStart_toStartOf="@+id/S1L3_B8"
        app:layout_constraintTop_toBottomOf="@+id/S1L3_B8" />
