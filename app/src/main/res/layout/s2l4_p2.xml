<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:theme="@style/LB_90">

    <Button
        android:id="@+id/S2L4_p2_b4"
        android:layout_marginTop="75dp"
        app:layout_constraintEnd_toEndOf="@+id/S2L4_p2_b1"
        app:layout_constraintStart_toStartOf="@+id/S2L4_p2_b1"
        app:layout_constraintTop_toBottomOf="@+id/S2L4_p2_b1" />

    <Button
        android:id="@+id/S2L4_p2_b6"
        android:layout_marginTop="75dp"
        app:layout_constraintEnd_toEndOf="@+id/S2L4_p2_b2"
        app:layout_constraintStart_toStartOf="@+id/S2L4_p2_b2"
        app:layout_constraintTop_toBottomOf="@+id/S2L4_p2_b2" />

    <Button
        android:id="@+id/S2L4_p2_b2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.25" />

    <Button
        android:id="@+id/S2L4_p2_b5"
        android:layout_marginTop="75dp"
        app:layout_constraintEnd_toEndOf="@+id/S2L4_p2_b3"
