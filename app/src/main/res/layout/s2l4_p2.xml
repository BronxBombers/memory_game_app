        android:id="@+id/S2L4_p2_b10"
        app:layout_constraintBottom_toTopOf="@+id/S2L4_p2_b2"
        app:layout_constraintEnd_toEndOf="@+id/S2L4_p2_b2"
        app:layout_constraintStart_toStartOf="@+id/S2L4_p2_b2"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/swipe_indicator"
        android:layout_width="100dp"
        android:layout_height="125dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.015"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.617"
        app:srcCompat="@drawable/swipe_indicator_left" />

